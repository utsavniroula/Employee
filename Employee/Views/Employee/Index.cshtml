@model IEnumerable<EmployeeInformation>

@{
}

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Address</th>
            <th>Salary</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var employee in Model)
        {
            <tr>
                <td>@employee.Name</td>
                <td>@employee.Address</td>
                <td>@employee.Salary</td>
                <td>
                    @Html.ActionLink("Options","Details","Employee",new { id = employee.Id},null)
                </td>
            </tr>
        }
    </tbody>
</table>